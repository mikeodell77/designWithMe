<h1>Log In</h1>

<%= form_tag sessions_path do %>


    <div class="large-8 columns">
      <%= label_tag :email %>
      <%= text_field_tag :email, params[:email] %>
    </div>

    <div class="large-8 columns">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>

    <div class="large-4 large-centered columns">
      <%= submit_tag "Log In", class: "small button" %>
    </div>
<% end %>